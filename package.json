    "next-redux-wrapper": "^7.0.5",
    "next-seo": "^5.5.0",
    "next-share": "^0.13.0",
    "react": "18.1.0",
    "react-bootstrap": "^2.5.0",
    "react-dom": "18.1.0",
    "react-infinite-scroll-component": "^6.1.0",
    "react-owl-carousel": "^2.3.3",
    "react-redux": "^8.0.2",
    "react-search-autocomplete": "^7.2.2",
